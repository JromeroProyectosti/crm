{% extends 'base.html.twig' %}

{% block title %}Cobranza{% endblock %}

{% block body %}
    <h1>Cobranza</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cobranza.id }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{{ cobranza.fecha ? cobranza.fecha|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>FechaCompromiso</th>
                <td>{{ cobranza.fechaCompromiso ? cobranza.fechaCompromiso|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Observacion</th>
                <td>{{ cobranza.observacion }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('cobranza_index') }}">back to list</a>

    <a href="{{ path('cobranza_edit', {'id': cobranza.id}) }}">edit</a>

    {{ include('cobranza/_delete_form.html.twig') }}
{% endblock %}
